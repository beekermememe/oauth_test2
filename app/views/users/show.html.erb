<% title "User" %>

<p>
  <strong>Google:</strong>
  <%= @user.google_id %>
</p>
<p>
  <strong>Google Picture:</strong>
  <%= @user.google_picture %>
</p>
<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>
<p>
  <strong>Google Email:</strong>
  <%= @user.google_email %>
</p>
<p>
  <strong>Google First Name:</strong>
  <%= @user.google_first_name %>
</p>
<p>
  <strong>Google Last Name:</strong>
  <%= @user.google_last_name %>
</p>

<p>
  <%= link_to "Edit", edit_user_path(@user) %> |
  <%= link_to "Destroy", @user, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", users_path %>
</p>
